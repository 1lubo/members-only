
<%= form_with model: article do |f|  %>



  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title %>    
   
  </div>

  <div>    
  <%= f.label :text %><br>
    <%= f.text_area :text, rows: 10 %><br>   
    
  </div>

  <div>
    <%= f.submit %>
  </div>
  <%= link_to 'Back', :back %>
<% end %>

